# Define here the models for your scraped items
#
# See documentation in:
# https://docs.scrapy.org/en/latest/topics/items.html

from __future__ import absolute_import


import scrapy


class ArticleItem(scrapy.Item):
    url = scrapy.Field() #author
    domain = scrapy.Field()
    datetime_scraped = scrapy.Field()
    published_date = scrapy.Field()
    scraped_headline = scrapy.Field()
    summarised_headline = scrapy.Field()
    scraped_content = scrapy.Field()
    summarised_content = scrapy.Field()
    related_links = scrapy.Field()
    #industry_id = scrapy.Field() 
    #sector_id = scrapy.Field() 
    pass
